<?xml version="1.0" encoding="UTF-8"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="N1" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <name><text>SimplePetriNet</text></name>

    <page id="page1">

      <!-- Places -->
      <place id="s1">
        <name><text>s1</text></name>
        <initialMarking><text>1</text></initialMarking>
      </place>

      <place id="s2">
        <name><text>s2</text></name>
        <initialMarking><text>0</text></initialMarking>
      </place>

      <place id="s3">
        <name><text>s3</text></name>
        <initialMarking><text>1</text></initialMarking>
      </place>

      <place id="s4">
        <name><text>s4</text></name>
        <initialMarking><text>0</text></initialMarking>
      </place>

      <!-- Transition -->
      <transition id="t">
        <name><text>t</text></name>
      </transition>

      <!-- Arcs -->
      <arc id="a1" source="s1" target="t"/>
      <arc id="a2" source="s3" target="t"/>
      <arc id="a3" source="t" target="s2"/>
      <arc id="a4" source="t" target="s4"/>

    </page>
  </net>
</pnml>
