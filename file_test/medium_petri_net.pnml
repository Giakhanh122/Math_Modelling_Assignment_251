<?xml version="1.0" encoding="UTF-8"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="net1" type="http://www.pnml.org/version-2009/grammar/ptnet">
    
    <!-- Places -->
    <place id="p1">
      <initialMarking><text>1</text></initialMarking>
    </place>
    <place id="p2">
      <initialMarking><text>0</text></initialMarking>
    </place>
    <place id="p3">
      <initialMarking><text>1</text></initialMarking>
    </place>
    <place id="p4">
      <initialMarking><text>0</text></initialMarking>
    </place>
    <place id="p5">
      <initialMarking><text>0</text></initialMarking>
    </place>
    <place id="p6">
      <initialMarking><text>1</text></initialMarking>
    </place>
    <place id="p7">
      <initialMarking><text>0</text></initialMarking>
    </place>
    <place id="p8">
      <initialMarking><text>1</text></initialMarking>
    </place>
    <place id="p9">
      <initialMarking><text>0</text></initialMarking>
    </place>
    <place id="p10">
      <initialMarking><text>0</text></initialMarking>
    </place>
    <place id="p11">
      <initialMarking><text>1</text></initialMarking>
    </place>
    <place id="p12">
      <initialMarking><text>0</text></initialMarking>
    </place>

    <!-- Transitions -->
    <transition id="t1"><name><text>T1</text></name></transition>
    <transition id="t2"><name><text>T2</text></name></transition>
    <transition id="t3"><name><text>T3</text></name></transition>
    <transition id="t4"><name><text>T4</text></name></transition>
    <transition id="t5"><name><text>T5</text></name></transition>
    <transition id="t6"><name><text>T6</text></name></transition>
    <transition id="t7"><name><text>T7</text></name></transition>

    <!-- Arcs -->
    <arc id="a1" source="p1" target="t1">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a2" source="p2" target="t1">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a3" source="t1" target="p3">
      <inscription><text>1</text></inscription>
    </arc>

    <arc id="a4" source="p3" target="t2">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a5" source="t2" target="p4">
      <inscription><text>1</text></inscription>
    </arc>

    <arc id="a6" source="p4" target="t3">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a7" source="t3" target="p5">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a8" source="p5" target="t4">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a9" source="t4" target="p1">
      <inscription><text>1</text></inscription>
    </arc>

    <arc id="a10" source="p6" target="t5">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a11" source="t5" target="p7">
      <inscription><text>1</text></inscription>
    </arc>

    <arc id="a12" source="p7" target="t6">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a13" source="t6" target="p8">
      <inscription><text>1</text></inscription>
    </arc>

    <arc id="a14" source="p8" target="t7">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a15" source="t7" target="p9">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a16" source="p9" target="t5">
      <inscription><text>1</text></inscription>
    </arc>

    <!-- Some extra arcs to create loops & potential deadlocks -->
    <arc id="a17" source="p10" target="t3">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a18" source="t3" target="p11">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a19" source="p11" target="t6">
      <inscription><text>1</text></inscription>
    </arc>
    <arc id="a20" source="t6" target="p12">
      <inscription><text>1</text></inscription>
    </arc>
    
  </net>
</pnml>
